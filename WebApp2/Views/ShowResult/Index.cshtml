@{
    ViewData["Title"] = "Employee Result";
}



<h4>Employee Result</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Index">
           
            <div class="form-group col-md-4 ">
                <label class="control-label">Employee ID</label>
                <input id="EmployeeID" type="text" class="form-control" />
               
            </div>
            <div class="form-group">
                <a href="#" class="btn btn-primary" onclick="ShowSalarywithBonus(id)"> Show Salary with Bonus</a>
                <a href="#" class="btn btn-info" onclick="HighlightSalary(id)"> Highlight Salary</a>
                <a href="#" class="btn btn-info" onclick="HighlightName(id)"> Highlight Name</a>
            </div>
          
        </form>
    </div>
</div>